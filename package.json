{
  "name": "@wangyang2010344/quill-image-resize-module",
  "version": "3.0.0",
  "description": "A module for Quill rich text editor to allow images to be resized.",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wy2010344/quill-image-resize-module"
  },
  "keywords": [
    "quill",
    "quilljs",
    "image",
    "resize",
    "sizer"
  ],
  "author": "@kensnyder+@wangyang2010344",
  "peerDependencies": {
    "lodash": "*",
    "quill": "*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "contributors": [
    {
      "name": "Wikia",
      "url": "https://github.com/Wikia"
    }
  ],
  "bugs": {
    "url": "https://github.com/wy2010344/quill-image-resize-module/issues"
  },
  "homepage": "https://github.com/wy2010344/quill-image-resize-module#readme",
  "module": "dist/quill-image-resize-module.esm.js",
  "size-limit": [
    {
      "path": "dist/quill-image-resize-module.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/quill-image-resize-module.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/quill": "^2.0.9",
    "husky": "^7.0.4",
    "@types/lodash": "^4.14.186",
    "quill": "*",
    "lodash": "*",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2"
  }
}